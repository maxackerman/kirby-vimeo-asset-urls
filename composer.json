{
    "name": "violetoffice/vimeoassets",
    "description": "Kirby Plugin to get Vimeo asset URLs through the Vimeo API",
    "type": "kirby-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Ackerman",
            "email": "max@violetoffice.com"
        }
    ],
    "require": {
        "getkirby/composer-installer": "^1.1",
        "vimeo/vimeo-api": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Violetoffice\\Vimeoassets\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
